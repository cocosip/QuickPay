<QuickPayConfig>
  <Alipay>
    <![CDATA[支付宝网关]]>
    <Gateway>https://openapi.alipay.com/gateway.do</Gateway>
    <![CDATA[本地IP地址]]>
    <LocalAddress>127.0.0.1</LocalAddress>
    <![CDATA[网址(如果前后端分离,为前端的网址)]]>
    <WebGateway>http://127.0.0.1</WebGateway>
    <![CDATA[支付宝支付异步通知默认网关地址]]>
    <NotifyGateway>http://127.0.0.1</NotifyGateway>
    <![CDATA[通知关联Url]]>
    <NotifyRealateUrl>/Notify/Alipay</NotifyRealateUrl>
    <![CDATA[二维码扫码支付异步通知关联Url]]>
    <QrcodeNotifyRelateUrl>/Notify/Alipay/QrcodeNotify</QrcodeNotifyRelateUrl>
    <![CDATA[条码支付异步通知关联Url]]>
    <BarcodeNotifyRelateUrl>/Notify/Alipay/BarcodeNotify</BarcodeNotifyRelateUrl>
    <![CDATA[请求数据格式,JSON]]>
    <Format>JSON</Format>
    <![CDATA[版本号]]>
    <Version>1.0</Version>
    <![CDATA[默认支付使用的App名称]]>
    <DefaultAppName>App1</DefaultAppName>
    <Apps>
      <AlipayApp>
        <![CDATA[应用AppId]]>
        <AppId>AppId1</AppId>
        <![CDATA[应用类型,填1]]>
        <AppTypeId>1</AppTypeId>
        <![CDATA[编码,utf-8]]>
        <Charset>utf-8</Charset>
        <![CDATA[App名称]]>
        <Name>App1</Name>
        <![CDATA[App私钥]]>
        <PrivateKey>私钥</PrivateKey>
        <![CDATA[App公钥]]>
        <PublicKey>公钥</PublicKey>
        <![CDATA[签名类型,RSA]]>
        <SignType>RSA</SignType>
        <EnableEncrypt>False</EnableEncrypt>
        <EncryptType>AES</EncryptType>
        <EncryptKey>123</EncryptKey>
      </AlipayApp>
    </Apps>
  </Alipay>
  <WechatPay>
    <![CDATA[本地IP地址]]>
    <LocalAddress>8.8.8.8</LocalAddress>
    <![CDATA[本地网关地址]]>
    <WebGateway>http://127.0.0.1</WebGateway>
    <![CDATA[默认通知网关,本地服务器地址]]>
    <NotifyGateway>http://127.0.0.1</NotifyGateway>
    <![CDATA[异步通知关联地址]]>
    <NotifyRealateUrl>/Notify/WechatPay</NotifyRealateUrl>
    <![CDATA[默认App名称]]>
    <DefaultAppName>App1</DefaultAppName>
    <![CDATA[签名类型,MD5]]>
    <SignType>MD5</SignType>
    <![CDATA[SSL证书]]>
    <SslPassword></SslPassword>
    <Apps>
      <WechatPayApp>
        <![CDATA[应用AppId]]>
        <AppId>AppId</AppId>
        <![CDATA[商户号]]>
        <MchId>MchId</MchId>
        <![CDATA[应用名称]]>
        <Name>App1</Name>
        <![CDATA[应用类型,1-公众号,2-应用]]>
        <AppTypeId>2</AppTypeId>
        <![CDATA[加密AppSecret]]>
        <Appsecret>appsecret</Appsecret>
        <![CDATA[密钥Key]]>
        <Key>密钥</Key>
        <NativeMobileInfo>
          <![CDATA[Ios的BundleId]]>
          <BundleId>1111111</BundleId >
          <![CDATA[Ios应用名]]>
          <IosName>Ios应用名</IosName>
          <![CDATA[安卓名称]]>
          <AndroidName>Android01</AndroidName>
          <![CDATA[安卓包名]]>
          <PackageName>cn.android.0001p</PackageName>
          <![CDATA[移动端名称]]>
          <WapName>Wap001</WapName>
          <![CDATA[移动端Url地址]]>
          <WapUrl>http://wap.b1.com</WapUrl>
        </NativeMobileInfo>
      </WechatPayApp>
    </Apps>
  </WechatPay>
</QuickPayConfig>
